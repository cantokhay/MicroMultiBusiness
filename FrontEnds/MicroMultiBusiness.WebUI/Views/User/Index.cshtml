@model UserDetailVM
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br /><br /><br />
    <h3>Profile Details</h3>
    <div class="alert alert-info">User Id : @Model.Id</div>
    <div class="alert alert-warning">User Name : @Model.UserName</div>
    <div class="alert alert-info">Email Address : @Model.Email</div>
    <div class="alert alert-warning">Full Name : @Model.Name @Model.LastName</div>
</div>